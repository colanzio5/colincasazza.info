{
  "name": "colincasazza.info",
  "version": "0.0.0",
  "scripts": {
    "build:wasm": "wasm-pack build wasm-lib -t web",
    "build:ts": "vue-tsc --noEmit && vite build",
    "deploy": "npm run build && firebase deploy",
    "dev": "vite",
    "build": "run-p type-check build-only",
    "preview": "vite preview --port 4173",
    "test:unit": "vitest --environment jsdom",
    "test:e2e": "start-server-and-test preview http://127.0.0.1:4173/ 'cypress open --e2e'",
    "test:e2e:ci": "start-server-and-test preview http://127.0.0.1:4173/ 'cypress run --e2e'",
    "build-only": "vite build",
    "type-check": "vue-tsc --noEmit -p tsconfig.vitest.json --composite false",
    "lint": "eslint . --ext .vue,.js,.jsx,.cjs,.mjs,.ts,.tsx,.cts,.mts --fix --ignore-path .gitignore"
  },
  "dependencies": {
    "@dimforge/rapier2d": "^0.8.0-alpha.0",
    "@dimforge/rapier2d-compat": "^0.8.0-alpha.0",
    "@tailwindcss/aspect-ratio": "^0.4.0",
    "@tailwindcss/postcss7-compat": "^2.2.17",
    "@types/dat.gui": "^0.7.7",
    "@types/lodash": "^4.14.178",
    "dat.gui": "^0.7.7",
    "esm": "^3.2.25",
    "lodash": "^4.17.21",
    "mitt": "^3.0.0",
    "postcss-import": "^14.0.2",
    "register-service-worker": "^1.7.2",
    "three": "^0.136.0",
    "v-dragged": "^0.0.5",
    "vite-tsconfig-paths": "^3.3.17",
    "vue": "^3.2.36",
    "vue-class-component": "^8.0.0-rc.1",
    "vue-drag-resize": "^2.0.3",
    "vue-property-decorator": "10.0.0-rc.3",
    "vue-router": "^4.0.15",
    "vue3-touch-events": "^4.1.0",
    "vuex": "^4.0.2",
    "vuex-class-component": "^2.3.6"
  },
  "devDependencies": {
    "@originjs/vite-plugin-commonjs": "^1.0.3",
    "@rushstack/eslint-patch": "^1.1.0",
    "@types/jsdom": "^16.2.14",
    "@types/node": "^16.11.36",
    "@types/tailwindcss": "^3.0.3",
    "@types/three": "^0.136.1",
    "@types/three-dat.gui": "^2.0.1",
    "@vitejs/plugin-vue": "^2.3.3",
    "@vue/eslint-config-prettier": "^7.0.0",
    "@vue/eslint-config-typescript": "^10.0.0",
    "@vue/test-utils": "^2.0.0",
    "@vue/tsconfig": "^0.1.3",
    "autoprefixer": "^10.4.7",
    "cypress": "^10.0.2",
    "eslint": "^8.5.0",
    "eslint-plugin-cypress": "^2.12.1",
    "eslint-plugin-vue": "^8.2.0",
    "jsdom": "^19.0.0",
    "npm-run-all": "^4.1.5",
    "postcss": "^8.4.14",
    "prettier": "^2.5.1",
    "start-server-and-test": "^1.14.0",
    "tailwindcss": "^3.1.1",
    "typescript": "~4.7.2",
    "vite": "^2.9.9",
    "vite-plugin-wasm-pack": "^0.1.9",
    "vitest": "^0.13.0",
    "vue-tsc": "^0.35.2"
  }
}
